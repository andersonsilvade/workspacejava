
import java.util.List;


public class Norte implements ImprimirVitimaRegiao{

	public void imprimirVitimaRegia(List<Acidente> v){
			String n = "Norte";
	        if(!v.isEmpty()){
	        	System.out.println();
	              for(Object i : v){
			      Acidente acid = (Acidente)i;
			      if(acid.getTipo().equals(n)){
			      System.out.println("Total de acidentes de "+acid.getTipo()+" na região Norte "+acid.getReg().getNorte());
			      }
		                          
		                   }
	                }
}
}

